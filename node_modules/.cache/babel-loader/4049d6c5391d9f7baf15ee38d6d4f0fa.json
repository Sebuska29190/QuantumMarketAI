{"ast":null,"code":"import{createSlice}from\"@reduxjs/toolkit\";import{tap}from\"rxjs\";import{exchangesService}from\"../services\";var initState={data:[]};var exchangesSlice=createSlice({name:'exchanges',initialState:initState,reducers:{setExchanges:function setExchanges(state,action){state.data=action.payload;}}});var setExchanges=exchangesSlice.actions.setExchanges;var retrieveExchangesAction=function retrieveExchangesAction(state,dispatch,queryParams){return exchangesService.retrieve(queryParams).pipe(tap(function(data){if(queryParams['page']===1){dispatch(setExchanges(data));}else{dispatch(setExchanges(state.data.concat(data)));}}));};export{exchangesSlice,retrieveExchangesAction};","map":null,"metadata":{},"sourceType":"module"}