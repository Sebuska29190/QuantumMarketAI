{"ast":null,"code":"import{createSlice}from\"@reduxjs/toolkit\";import{tap}from\"rxjs\";import{coinsService}from\"../services\";var initState={data:[]};var coinsSlice=createSlice({name:'coins',initialState:initState,reducers:{setCoins:function setCoins(state,action){state.data=action.payload;}}});var setCoins=coinsSlice.actions.setCoins;var retrieveCoinsAction=function retrieveCoinsAction(state,dispatch,queryParams){return coinsService.retrieve(queryParams).pipe(tap(function(data){if(queryParams['offset']===0){dispatch(setCoins(data));}else{dispatch(setCoins(state.data.concat(data)));}}));};export{coinsSlice,retrieveCoinsAction};","map":null,"metadata":{},"sourceType":"module"}