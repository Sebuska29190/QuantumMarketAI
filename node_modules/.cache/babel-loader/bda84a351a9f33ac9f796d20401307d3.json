{"ast":null,"code":"import{createSlice}from\"@reduxjs/toolkit\";import{tap}from\"rxjs\";import{newsService}from\"../services\";var initState={data:[]};var newsSlice=createSlice({name:'news',initialState:initState,reducers:{setNews:function setNews(state,action){state.data=action.payload;}}});var setNews=newsSlice.actions.setNews;var retrieveNewsAction=function retrieveNewsAction(state,dispatch,queryParams){return newsService.retrieve(queryParams).pipe(tap(function(data){if(queryParams['page']===1){dispatch(setNews(data));}else{dispatch(setNews(state.data.concat(data)));}}));};export{newsSlice,retrieveNewsAction};","map":null,"metadata":{},"sourceType":"module"}