{"ast":null,"code":"import{createSlice}from\"@reduxjs/toolkit\";import{tap}from\"rxjs\";import{coinsService,exchangesService,newsService}from\"../services\";var initState={coins:[],exchanges:[],news:[]};var homeSlice=createSlice({name:'home',initialState:initState,reducers:{setCoins:function setCoins(state,action){state.coins=action.payload;},setExchanges:function setExchanges(state,action){state.exchanges=action.payload;},setNews:function setNews(state,action){state.news=action.payload;}}});var _homeSlice$actions=homeSlice.actions,setCoins=_homeSlice$actions.setCoins,setExchanges=_homeSlice$actions.setExchanges,setNews=_homeSlice$actions.setNews;var retrieveCoinsAction=function retrieveCoinsAction(dispatch,queryParams){return coinsService.retrieve(queryParams).pipe(tap(function(data){dispatch(setCoins(data.slice(0,12)));}));};var retrieveExchangesAction=function retrieveExchangesAction(dispatch){return exchangesService.retrieve({page:1}).pipe(tap(function(data){dispatch(setExchanges(data.slice(0,12)));}));};var retrieveNewsAction=function retrieveNewsAction(dispatch){return newsService.retrieve({page:1}).pipe(tap(function(data){dispatch(setNews(data.slice(0,12)));}));};export{homeSlice,retrieveCoinsAction,retrieveExchangesAction,retrieveNewsAction};","map":null,"metadata":{},"sourceType":"module"}